package com.javalec.ex;

import org.springframework.stereotype.Service;

@Service
public class OtherStudent {
	
	private String name;
	private int age;
	
	public OtherStudent(String name , int age){
		this.name = name;
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public int getAge() {
		return age;
	}
	
	@PostConstruct
	public void init(){
		System.out.println("init()");
	}
}
